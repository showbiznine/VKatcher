                               Grid.Row="3"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec34"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="4"
                               Margin="5"
                               Grid.Row="3"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec40"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Margin="5"
                               Grid.Row="4"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec41"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="1"
                               Margin="5"
                               Grid.Row="4"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec42"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="2"
                               Margin="5"
                               Grid.Row="4"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec43"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="3"
                               Margin="5"
                               Grid.Row="4"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec44"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="4"
                               Margin="5"
                               Grid.Row="4"
                               RenderTransformOrigin="0.5,0.5">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec21b"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="1"
                               Margin="5"
                               Grid.Row="2"
                               RenderTransformOrigin="0.5,0.5"
                               Visibility="Collapsed"
                               d:IsHidden="True">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec12b"
                               Height="6"
                               Width="6"
                               StrokeThickness="0"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Grid.Column="2"
                               Margin="5"
                               Grid.Row="1"
                               RenderTransformOrigin="0.5,0.5"
                               Visibility="Collapsed"
                               d:IsHidden="True">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <TextBlock x:Name="lblTrackTime"
                               Margin="0"
                               Grid.RowSpan="5"
                               TextWrapping="Wrap"
                               Text="{Binding _trackTime}"
                               d:LayoutOverrides="Width, Height"
                               Grid.ColumnSpan="5"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="20"
                               RenderTransformOrigin="0.5,0.5">
                        <TextBlock.RenderTransform>
                            <CompositeTransform ScaleX="0"
                                                ScaleY="0" />
                        </TextBlock.RenderTransform>
                        <TextBlock.Foreground>
                            <SolidColorBrush Color="{ThemeResource SystemAltMediumHighColor}" />
                        </TextBlock.Foreground>
                    </TextBlock>
                    <TextBlock x:Name="lblTrackStatus"
                               TextWrapping="Wrap"
                               Grid.ColumnSpan="5"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="48"
                               RenderTransformOrigin="0.5,0.5"
                               FontFamily="Segoe MDL2 Assets"
                               Grid.RowSpan="5"
                               Text="{Binding _trackTime}">
                        <TextBlock.RenderTransform>
                            <CompositeTransform ScaleX="0"
                                                ScaleY="0" />
                        </TextBlock.RenderTransform>
                        <TextBlock.Foreground>
                            <SolidColorBrush Color="{ThemeResource SystemAltMediumHighColor}" />
                        </TextBlock.Foreground>
                    </TextBlock>
                </Grid>
            </Button>
            <StackPanel x:Name="stkControlButtons"
                        Grid.Column="3"
                        VerticalAlignment="Center"
                        Margin="0,-11,10,26"
                        Orientation="Horizontal"
                        Grid.Row="1">
                <Button x:Name="btnPrev"
                        Content="&#xE100;"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontFamily="Segoe MDL2 Assets"
                        Background="Transparent"
                        FontSize="26"
                        Command="{Binding SkipPreviousCommand}">
                    <Button.Foreground>
                        <SolidColorBrush Color="{ThemeResource SystemAltHighColor}" />
                    </Button.Foreground>
                </Button>
                <Button x:Name="btnPlayPause"
                        Content="&#xE102;"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="Transparent"
                        FontFamily="Segoe MDL2 Assets"
                        FontSize="26"
                        Margin="12,0"
                        Command="{Binding PlayPauseCommand}">
                    <Button.Foreground>
                        <SolidColorBrush Color="{ThemeResource SystemAltHighColor}" />
                    </Button.Foreground>
                </Button>
                <Button x:Name="btnNext"
                        Content="&#xE101;"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontFamily="Segoe MDL2 Assets"
                        Background="Transparent"
                        FontSize="26"
                        Command="{Binding SkipNextCommand}">
                    <Button.Foreground>
                        <SolidColorBrush Color="{ThemeResource SystemAltHighColor}" />
                    </Button.Foreground>
                </Button>
            </StackPanel>
            <TextBlock x:Name="textBlock"
                       VerticalAlignment="Center"
                       FontSize="16"
                       RenderTransformOrigin="0.5,0.5"
                       Grid.Column="1"
                       Margin="12,0,0,0"
                       HorizontalAlignment="Left"
                       Grid.RowSpan="2"
                       TextTrimming="CharacterEllipsis">
                <TextBlock.RenderTransform>
                    <CompositeTransform />
                </TextBlock.RenderTransform>
                <TextBlock.Foreground>
                    <SolidColorBrush Color="#FFFFFFFF" />
                </TextBlock.Foreground>
                    <Run Text="{Binding _currentTrack.title}"
                         FontWeight="Bold"
                         FontSize="20" />                
                    <LineBreak />
                    <Run Text="// "
                         FontWeight="Light"/>
                    <Run Text="{Binding _currentTrack.artist}"
                         FontWeight="SemiBold" />
            </TextBlock>
        </Grid>
    </Grid>
</Page>
