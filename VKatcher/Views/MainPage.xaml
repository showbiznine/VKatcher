                               Width="6"
                               Height="6"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               RenderTransformOrigin="0.5,0.5"
                               StrokeThickness="0">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec01"
                               Grid.Column="1"
                               Width="6"
                               Height="6"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               RenderTransformOrigin="0.5,0.5"
                               StrokeThickness="0">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="rec02"
                               Grid.Column="2"
                               Width="6"
                               Height="6"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               RenderTransformOrigin="0.5,0.5"
                               StrokeThickness="0">
                        <Rectangle.RenderTransform>
                            <CompositeTransform />
                        </Rectangle.RenderTransform>
                        <Rectangle.Fill>
                            <SolidColorBrush Color="#CCFFFFFF" />
