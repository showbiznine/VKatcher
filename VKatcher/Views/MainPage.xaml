        <VisualStateManager.VisualStateGroups>
            <VisualStateGroup x:Name="WindowWidth">
                <VisualState x:Name="Mobile">
                    <VisualState.Setters>
                        <Setter Target="grdSlider.(Grid.Column)" Value="0" />
                        <Setter Target="grdSlider.(Grid.ColumnSpan)" Value="4" />
                        <Setter Target="grdSlider.(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Value="-20" />
                        <Setter Target="grdSlider.(UIElement.Visibility)" Value="Collapsed" />
                        <Setter Target="stkControlButtons.(UIElement.Visibility)" Value="Collapsed" />
                        <Setter Target="btnSmart.(UIElement.Visibility)" Value="Visible" />
                    </VisualState.Setters>
                    <VisualState.StateTriggers>
                        <AdaptiveTrigger MinWindowWidth="100" />
                    </VisualState.StateTriggers>
                    <Storyboard>
                        <ColorAnimation d:IsOptimized="True"
                                        Storyboard.TargetName="lblCurrentTime"
                                        Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
                                        To="{ThemeResource SystemBaseHighColor}"
                                        Duration="0" />
                        <ColorAnimation d:IsOptimized="True"
                                        Storyboard.TargetName="lblTotalTime"
                                        Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
                                        To="{ThemeResource SystemBaseHighColor}"
                                        Duration="0" />
                        <DoubleAnimation d:IsOptimized="True"
                                         Storyboard.TargetName="grdSlider"
                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                         To="0"
                                         Duration="0" />
                    </Storyboard>
                </VisualState>
                <VisualState x:Name="Tablet">
                    <VisualState.Setters>
                        <Setter Target="btnSmart.(UIElement.Visibility)" Value="Collapsed" />
                        <Setter Target="stkControlButtons.(UIElement.Visibility)" Value="Visible" />
                        <Setter Target="grdSlider.(UIElement.Visibility)">
                            <Setter.Value>
                                <Visibility>Visible</Visibility>
                            </Setter.Value>
                        </Setter>
                    </VisualState.Setters>
                    <VisualState.StateTriggers>
                        <AdaptiveTrigger MinWindowWidth="700" />
                    </VisualState.StateTriggers>
                    <Storyboard>
                        <DoubleAnimation d:IsOptimized="True"
                                         Storyboard.TargetName="grdSlider"
                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                         To="1"
                                         Duration="0" />
                    </Storyboard>
                </VisualState>
            </VisualStateGroup>
        </VisualStateManager.VisualStateGroups>
