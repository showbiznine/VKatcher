    <Page.DataContext>
        <Binding Path="Main" Source="{StaticResource Locator}" />
    </Page.DataContext>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Controls:SwipeableSplitView x:Name="mySplitView"
                                     Margin="0"
                                     IsSwipeablePaneOpen="{Binding IsMenuOpen, Mode=TwoWay}"
                                     MaxOpacity="0.5">
            <Controls:SwipeableSplitView.Pane>
                <Grid Background="{StaticResource Med_Gray}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <StackPanel x:Name="stkSearch"
                                Margin="12,32,12,0"
                                VerticalAlignment="Top">
                        <TextBlock x:Name="textBlock2"
                                   Text="Search"
                                   TextWrapping="Wrap" />
                        <AutoSuggestBox x:Name="asbSearch"
                                        VerticalAlignment="Top"
                                        QueryIcon="Find"
                                        Text="{Binding _searchQuery, Mode=TwoWay}">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="QuerySubmitted">
                                    <Core:InvokeCommandAction Command="{Binding SearchCommand}" />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>
                        </AutoSuggestBox>
                    </StackPanel>
                    <Controls:HamburgerMenuItem x:Name="hmbSettings"
                                                Grid.Row="1"
                                                Margin="0,12,0,0"
                                                VerticalContentAlignment="Stretch"
                                                Content="Settings"
                                                GroupName="menu"
                                                Icon=""
                                                Style="{StaticResource HamburgerMenuItemStyle1}">
                        <Interactivity:Interaction.Behaviors>
                            <Core:EventTriggerBehavior EventName="Checked">
                                <Core:InvokeCommandAction Command="{Binding GoToSettingsCommand}" />
                            </Core:EventTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>
                    </Controls:HamburgerMenuItem>
                    <Controls:HamburgerMenuItem x:Name="hmbMyMusic"
                                                Grid.Row="2"
                                                Content="My Music"
                                                GroupName="menu"
                                                Icon=""
                                                Style="{StaticResource HamburgerMenuItemStyle1}">
                        <Interactivity:Interaction.Behaviors>
                            <Core:EventTriggerBehavior EventName="Checked">
                                <Core:InvokeCommandAction Command="{Binding GoToMyMusicCommand}" />
                            </Core:EventTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>
                    </Controls:HamburgerMenuItem>

                    <!--<Button x:Name="button"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Stretch"
                            Margin="0"
                            d:LayoutOverrides="Width, LeftPosition, RightPosition"
                            Command="{Binding GoToSettingsCommand}"
                            Grid.Row="1" Background="Transparent" Visibility="Collapsed" >
                        <Grid/>
                    </Button>-->
                    <!--<Button x:Name="button_Copy"
                            Content="MY MUSIC"
                            VerticalAlignment="Bottom"
                            HorizontalAlignment="Center"
                            Margin="0,0,0,309"
                            Command="{Binding GoToMyMusicCommand}"
                            Grid.Row="2" Visibility="Collapsed" />-->
                </Grid>
            </Controls:SwipeableSplitView.Pane>
            <Grid>
                <Frame x:Name="myFrame" />
            </Grid>
        </Controls:SwipeableSplitView>
        <Grid x:Name="grdPlaybackControls"
              Grid.Row="2"
              Grid.ColumnSpan="2"
              Height="100"
              VerticalAlignment="Bottom"
              Background="#00000000"
              RenderTransformOrigin="0.5,0.5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" MinWidth="0" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="3*" />
                <RowDefinition Height="5*" />
            </Grid.RowDefinitions>
            <Grid.RenderTransform>
                <CompositeTransform TranslateY="100" />
            </Grid.RenderTransform>
            <Grid x:Name="grdExtras"
                  Grid.RowSpan="2"
                  Grid.ColumnSpan="4"
                  RenderTransformOrigin="0.5,0.5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100" />
                    <ColumnDefinition />
                    <ColumnDefinition Width="100" />
                </Grid.ColumnDefinitions>
                <Grid.RenderTransform>
                    <CompositeTransform />
                </Grid.RenderTransform>
                <Controls:BackDrop Grid.RowSpan="2"
                                   Grid.ColumnSpan="3"
                                   d:IsHidden="True"
                                   BlurAmount="6" />
                <Rectangle x:Name="recTint"
                           Grid.ColumnSpan="3"
                           Fill="{StaticResource Med_Gray}"
                           RenderTransformOrigin="0.5,0.5"
                           StrokeThickness="0">
                    <Rectangle.RenderTransform>
                        <CompositeTransform />
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Button x:Name="btnAlbum"
                        HorizontalAlignment="Center"
                        Command="{Binding GoToNowPlayingCommand}"
                        Style="{StaticResource SimpleButtonStyle}">
                    <Grid x:Name="grid"
                          HorizontalAlignment="Center"
                          VerticalAlignment="Center">
                        <Grid.RenderTransform>
                            <CompositeTransform />
                        </Grid.RenderTransform>
                        <TextBlock x:Name="textBlock1"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontFamily="Segoe MDL2 Assets"
                                   FontSize="56"
                                   Text="&#xE93C;"
                                   TextWrapping="Wrap"
                                   d:LayoutOverrides="Width">
                            <TextBlock.Foreground>
                                <SolidColorBrush Color="White" />
                            </TextBlock.Foreground>
                        </TextBlock>
                        <Image x:Name="imgPlayingAlbumArt"
                               Width="80"
                               Height="80"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               RenderTransformOrigin="0.5,0.5"
                               Source="{Binding _currentTrack.photo_url}"
                               d:LayoutOverrides="Width, LeftMargin, RightMargin, TopMargin, BottomMargin">
                            <Image.RenderTransform>
                                <CompositeTransform />
                            </Image.RenderTransform>
                        </Image>
                    </Grid>
                </Button>
            </Grid>
            <Grid x:Name="grdSlider"
                  Grid.RowSpan="2"
                  Grid.Column="2"
                  Margin="16,0"
                  VerticalAlignment="Center"
                  RenderTransformOrigin="0.5,0.5"
                  d:LayoutOverrides="Height">
                <Grid.RenderTransform>
                    <CompositeTransform />
                </Grid.RenderTransform>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <TextBlock x:Name="lblCurrentTime"
                           Margin="0,0,8,0"
                           HorizontalAlignment="Right"
                           VerticalAlignment="Center"
                           Text="{Binding _trackTime}"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="Height">
                    <TextBlock.Foreground>
                        <SolidColorBrush Color="{ThemeResource SystemAltMediumHighColor}" />
                    </TextBlock.Foreground>
                </TextBlock>
                <Slider x:Name="slider"
                        Grid.Column="1"
                        MinWidth="300"
                        Margin="8,0"
                        VerticalAlignment="Center"
                        Maximum="{Binding _currentTrack.duration}"
                        RenderTransformOrigin="0.5,0.5"
                        Style="{StaticResource TrackControlSliderStyle}"
                        ThumbToolTipValueConverter="{StaticResource TimespanStringConverter}"
                        Value="{Binding _trackPosition, Mode=TwoWay}">
                    <Interactivity:Interaction.Behaviors>
                        <Core:EventTriggerBehavior EventName="PointerCaptureLost">
                            <Core:InvokeCommandAction Command="{Binding SliderUpCommand}" />
                        </Core:EventTriggerBehavior>
                        <Core:EventTriggerBehavior EventName="PointerPressed">
                            <Core:InvokeCommandAction Command="{Binding SliderDownCommand}" />
                        </Core:EventTriggerBehavior>
                    </Interactivity:Interaction.Behaviors>
                    <Slider.RenderTransform>
                        <CompositeTransform />
                    </Slider.RenderTransform>
                </Slider>
                <TextBlock x:Name="lblTotalTime"
                           Grid.Column="2"
                           Margin="8,0,0,0"
                           HorizontalAlignment="Right"
                           VerticalAlignment="Center"
                           Text="{Binding _currentTrack.duration, Converter={StaticResource TimespanStringConverter}}"
                           TextWrapping="Wrap"
                           d:LayoutOverrides="Height">
                    <TextBlock.Foreground>
                        <SolidColorBrush Color="{ThemeResource SystemAltMediumHighColor}" />
                    </TextBlock.Foreground>
                </TextBlock>
            </Grid>
            <Button x:Name="btnSmart"
                    Grid.RowSpan="2"
                    Grid.Column="3"
                    Margin="10"
                    Padding="0"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Stretch"
                    Background="#00000000"
                    BorderThickness="0"
                    Command="{Binding PlayPauseCommand}"
                    ManipulationCompleted="btnSmart_ManipulationCompleted">
                <Button.RenderTransform>
                    <CompositeTransform />
                </Button.RenderTransform>
                <Interactivity:Interaction.Behaviors>
                    <Core:EventTriggerBehavior EventName="ManipulationCompleted">
                        <Core:InvokeCommandAction Command="{Binding DataContext.SmartSwipeCommand, ElementName=pageRoot}" />
                    </Core:EventTriggerBehavior>
                </Interactivity:Interaction.Behaviors>
